module examples/fasteffekt/benchmark

def innerBenchmarkLoop[T](iterations: Int){benchmark: ()=>T}{verifyResult: T => Boolean} = {
    each(0, iterations) {
        i =>
        if (not(verifyResult(benchmark()))) {
            panic("incorrect benchmark result")
        };
        ();
    }
}

interface Benchmark {
    def benchmark(): Int
    def verifyResult(result: Int): Boolean
}

